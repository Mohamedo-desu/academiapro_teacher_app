{
  "name": "academiapro_teacher_app",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "build:android": "clear && eas build --platform android --profile preview",
    "offline": "expo start --offline",
    "ota:android": "clear && eas update --branch preview --platform android",
    "ota:ios": "clear && eas update --branch preview --platform ios",
    "ota": "clear && eas update --branch preview",
    "build:web": "clear && npx expo export --platform web && eas deploy --prod",
    "release:web": "clear && eas deploy --prod",
    "upgrade": "clear && bun install expo@latest && bunx expo install --fix",
    "android:debug": "clear && bunx expo run:android --device --variant debug",
    "android:release": "clear && bunx expo run:android --device --variant Release",
    "ios:release": "bunx expo run:ios --configuration Release",
    "ios": "expo run:ios",
    "test": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "android": "expo run:android"
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-vector-icons/material-design-icons": "^12.4.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.25",
    "expo-build-properties": "~1.0.9",
    "expo-checkbox": "~5.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.13",
    "expo-web-browser": "~15.0.9",
    "immer": "^11.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.68.0",
    "react-native": "0.81.5",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^4.1.0",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-unistyles": "^3.0.18",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "typescript": "~5.9.2"
  },
